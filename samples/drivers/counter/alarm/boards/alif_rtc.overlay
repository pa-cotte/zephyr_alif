/*
 * Enabling rtc & disabling lptimer, utimer for this sample application
 */

&timer0 {
	status = "disabled";
};

&rtc0 {
	status = "okay";
};

&utimer0 {
	status = "disabled";

	counter {
		status = "disabled";
	}
};
